<div class="menu" id="menu">
<div class="members">
  <div class="container">
    <div class="row">
      <div class="col-sm-12 logo-menu">
        <div class="text-center menu-logo">
        </div>
      </div>
    </div>
        <div class="text-center">
          <h2>Appetizers</h2>
        </div>
    <div class="row">
      <% @appetizers.each do |appetizer|%>
      <% if appetizer.description.nil? %>
      <div class="col-xs-12 col-sm-6 menu-line dish">
        <div class="title">
          <h3><%= appetizer.name %></h3>
        </div>
        <div class="price">
          <p><%= appetizer.price %> ₪</p>
        </div>
        <br>
        <hr class="style-seven">
      </div>
      <% else %>
      <div class="col-xs-12 col-sm-6 menu-line menu">
         <div class="title">
          <h3><%= appetizer.name %></h3>
        </div>
        <div class="des">
          <p><%= appetizer.description %></p>
        </div>
         <div class="price">
          <p><%= appetizer.price %> ₪</p>
        </div>
        <hr class="style-seven">
      </div>
      <% end %>
      <% end %>
    </div>
</div>
</div>
</div>
</div>
<div class="menu-pic-2"></div>


